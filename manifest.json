{
  "manifest_version": 3,
  "name": "YouLyrics",
  "description": "Injects song lyrics in the description of the video",
  "version": "0.1.0",

  "content_scripts": [
    {
      "matches": ["https://*.youtube.com/*"],
      "js": ["dist/assets/index.js"],
      "css": ["dist/assets/index.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["icons/*"],
      "matches": ["<all_urls>"]
    }
  ]
}
